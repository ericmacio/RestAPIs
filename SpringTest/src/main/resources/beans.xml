<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- BEAN CONFIGURATION FILE -->
    <bean id="student_bean" class="com.eric.restapi.Student">
        <constructor-arg ref="address_bean"></constructor-arg>
        <property name="name" value="Eric"></property>
    </bean>
    <bean id="address_bean" class="com.eric.restapi.Address" >
        <property name="address" value="Grenoble"></property>
    </bean>

</beans>